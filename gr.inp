#----------------------------------------------------------------
# Mesh data
Add mesh: msh {
   Mesh file path:    mesh_tube_fsi/solid/mesh-complete.mesh.vtu
   Fiber direction file path: mesh_tube_fsi/solid/mesh-complete.mesh.vtu
   Number of variable wall properties: 7
   Variable wall properties file path: mesh_tube_fsi/solid/mesh-complete.mesh.vtu
   Add face: start {
      Face file path: mesh_tube_fsi/solid/mesh-surfaces/start.vtp
   }
   Add face: end {
      Face file path: mesh_tube_fsi/solid/mesh-surfaces/end.vtp
   }
   Add face: inside {
      Face file path: mesh_tube_fsi/solid/mesh-surfaces/interface.vtp
   }
   Add face: outside {
      Face file path: mesh_tube_fsi/solid/mesh-surfaces/outside.vtp
   }
   Add face: x_zero {
      Face file path: mesh_tube_fsi/solid/mesh-surfaces/x_zero.vtp
   }
   Add face: y_zero {
      Face file path: mesh_tube_fsi/solid/mesh-surfaces/y_zero.vtp
   }
   Mesh scale factor: 1.0
}

#----------------------------------------------------------------
# Equations
Add equation: struct {
   Coupled: 1
   Min iterations: 1
   Max iterations: 30
   Tolerance: 1e-8

   Constitutive model: GR_Equilibrated
   Density: 0.0
   Elasticity modulus: 1.0e-16
   Poisson ratio: 0.0

#   Constitutive model: stVK
#   Density: 0.0
#   Elasticity modulus: 1000.0
#   Poisson ratio: 0.0

   Output: Spatial {
      Displacement: t
      Velocity: t
      Jacobian: t
      Stress: t
      Strain: t
      Cauchy_stress: t
      VonMises_stress: t
      Acceleration: t
      Fiber_direction: t
   }

   LS type: GMRES
   {
      Tolerance:           1e-9
      Absolute tolerance:   1e-9
      Max iterations:      300
      Krylov space dimension: 300
#      Preconditioner:  Trilinos-GR
   }

   # plane strain
   Add BC: start {
      Type: Dir
      Value: 0.0
      Effective direction: (0, 0, 1)
      Impose on state variable integral: t
      Zero out perimeter: f
   }
   Add BC: end {
      Type: Dir
      Value: 0.0
      Effective direction: (0, 0, 1)
      Impose on state variable integral: t
      Zero out perimeter: f
   }

   # symmetry
   Add BC: x_zero {
      Type: Dir
      Value: 0.0
      Effective direction: (1, 0, 0)
      Impose on state variable integral: t
      Zero out perimeter: f
   }
   Add BC: y_zero {
      Type: Dir
      Value: 0.0
      Effective direction: (0, 1, 0)
      Impose on state variable integral: t
      Zero out perimeter: f
   }

   # step 1: pre-stressing
   Add BC: inside {
      Type: Neu
      Time dependence: Unsteady
      Temporal values file path: load_pre.dat
      Ramp function: t
      Follower pressure load: t
   }

   # step 2: g&r
   Add BC: inside {
      Type: Neu
      Time dependence: Unsteady
      Temporal values file path: load_gr.dat
      Ramp function: t
      Follower pressure load: t
   }
}
