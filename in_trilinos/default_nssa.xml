<ParameterList name="MultiLevelPreconditioner">
<!--Output-->
<Parameter name="ML output" type="int" value="0"/>
<!--General-->
<Parameter name="max levels" type="int" value="10"/>
<Parameter name="prec type" type="string" value="MGW"/>
<Parameter name="increasing or decreasing" type="string" value="increasing"/>
<!--Grid Transfer-->
<Parameter name="aggregation: type" type="string" value="Uncoupled-MIS"/>
<Parameter name="energy minimization: enable" type="bool" value="true"/>
<Parameter name="eigen-analysis: type" type="string" value="power-method"/>
<Parameter name="eigen-analysis: iterations" type="int" value="20"/>
<!--Smoothing-->
<Parameter name="smoother: sweeps" type="int" value="4"/>
<Parameter name="smoother: damping factor" type="double" value="0.67"/>
<Parameter name="smoother: pre or post" type="string" value="post"/>
<Parameter name="smoother: type" type="string" value="symmetric Gauss-Seidel"/>
<!--Coarse Solution-->
<Parameter name="coarse: type" type="string" value="Amesos-KLU"/>
<Parameter name="coarse: max size" type="int" value="256"/>
</ParameterList>
