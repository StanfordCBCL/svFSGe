<ParameterList name="MultiLevelPreconditioner">
<!--Output-->
<Parameter name="ML output" type="int" value="0"/>
<!-- <Parameter name="ML print final list" type="int" value="0"/>-->
<!-- <Parameter name="print hierarchy" type="int" value="-1"/>-->
<!--General-->
<Parameter name="max levels" type="int" value="1"/>
<Parameter name="prec type" type="string" value="MGV"/>
<!--Grid Transfer-->
<Parameter name="aggregation: type" type="string" value="Uncoupled"/>
<Parameter name="aggregation: damping factor" type="double" value="0.0"/>
<Parameter name="aggregation: block scaling" type="bool" value="true"/>
<!--<Parameter name="energy minimization: enable" type="bool" value="true"/>-->
<!--<Parameter name="eigen-analysis: type" type="string" value="Anorm"/>-->
<!--<Parameter name="eigen-analysis: iterations" type="int" value="20"/>-->
<!--Smoothing-->
<Parameter name="smoother: sweeps" type="int" value="2"/>
<Parameter name="smoother: damping factor" type="double" value="0.5"/>
<Parameter name="smoother: pre or post" type="string" value="post"/>
<Parameter name="smoother: type" type="string" value="Amesos-KLU"/>
<!--maxes out-->
<!--<Parameter name="smoother: type" type="string" value="Chebyshev"/>-->
<!--stops without converging-->
<!--<Parameter name="smoother: type" type="string" value="symmetric block Gauss-Seidel"/>-->
<!--<Parameter name="smoother: Chebyshev alpha" type="double" value="30"/>-->
<!--ifpack-->
<Parameter name="smoother: ifpack type" type="string" value="ILU"/>
<Parameter name="smoother: ifpack overlap" type="int" value="0"/>
<Parameter name="smoother: ifpack level-of-fill" type="double" value="8"/>
<!--<Parameter name="smoother: ifpack relative threshold" type="double" value="1.0"/>-->
<!--<Parameter name="smoother: ifpack absolute threshold" type="double" value="0.0"/>-->
<!--Coarse Solution-->
<Parameter name="coarse: type" type="string" value="Amesos-KLU"/>
<Parameter name="coarse: max size" type="int" value="128"/>
<!--<Parameter name="aggregation: threshold" type="double" value="1e-2"/>-->
<!--Repartition-->
<Parameter name="repartition: enable" type="int" value="1"/>
<Parameter name="repartition: Zoltan dimensions" type="int" value="3"/>
</ParameterList>

