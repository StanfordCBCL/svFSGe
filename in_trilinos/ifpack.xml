<ParameterList name="MultiLevelPreconditioner">
<Parameter name="ML output" type="int" value="0"/>
<!-- <Parameter name="ML print final list" type="int" value="0"/> -->
<!-- <Parameter name="print hierarchy" type="int" value="-1"/> -->
<!--General-->
<Parameter name="max levels" type="int" value="10"/>
<Parameter name="prec type" type="string" value="MGV"/>
<Parameter name="increasing or decreasing" type="string" value="increasing"/>
<!--Grid Transfer-->
<Parameter name="aggregation: type" type="string" value="Uncoupled"/>
<Parameter name="aggregation: damping factor" type="double" value="0.0"/>
<Parameter name="aggregation: block scaling" type="bool" value="true"/>
<Parameter name="energy minimization: enable" type="bool" value="true"/>
<Parameter name="eigen-analysis: type" type="string" value="Anorm"/>
<Parameter name="eigen-analysis: iterations" type="int" value="200"/>
<!--Smoothing-->
<Parameter name="smoother: sweeps" type="int" value="8"/>
<Parameter name="smoother: type" type="string" value="ILU"/>
<!--ifpack-->
<Parameter name="smoother: ifpack type" type="string" value="ILU"/>
<Parameter name="smoother: ifpack overlap" type="int" value="10"/>
<Parameter name="smoother: ifpack level-of-fill" type="double" value="20"/>
<Parameter name="smoother: ifpack relative threshold" type="double" value="1"/>
<Parameter name="smoother: ifpack absolute threshold" type="double" value="0"/>
<!--Coarse Solution-->
<Parameter name="coarse: type" type="string" value="Amesos-KLU"/>
<Parameter name="coarse: max size" type="int" value="256"/>
<Parameter name="aggregation: threshold" type="double" value="1e-4"/>
</ParameterList>
